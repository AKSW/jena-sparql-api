@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

@prefix r: <http://example.org/resource/> .
@prefix o: <http://example.org/ontology/> .


###############################################################################
# Main Dataset
###############################################################################

r:data-custom
  a o:Dataset ;
  .

###############################################################################
# File based distribution (e.g. for loading into a jena model or spark)
###############################################################################
r:dist-url-data-custom-1
  a o:UrlDistribution ;
  o:of r:data-custom ;
  o:url <custom.ttl>
  .

